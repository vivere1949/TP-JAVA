package tp3;

public class TP3 {

	public static void main(String[] args) {
		 int[] tableau = {10, 20, 30, 40, 50};

	        // Affichage des éléments du tableau
	        System.out.println("Les éléments du tableau sont :");
	        for (int i = 0; i < tableau.length; i++) {
	            System.out.println(tableau[i]);
	        }

	}

}
